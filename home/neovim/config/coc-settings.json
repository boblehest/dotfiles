{
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    }
  },
  "latex.forwardSearch.executable": "zathura",
  "latex.lint.onSave": false,
  "latex.forwardSearch.args": ["--synctex-forward", "%l:1:%f", "%p"]
}
